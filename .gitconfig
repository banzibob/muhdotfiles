[user]
	name = Sam Stachura
	email = sam.stachura@hiscox.com
[color]
	ui = true
[alias]
	s = status
	a = !git add . && git s
	cm = commit -m
	pushit = push -u origin --all
	rmrf = rm -rf
	mv = mv -v
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	required = true
	process = git-lfs filter-process
